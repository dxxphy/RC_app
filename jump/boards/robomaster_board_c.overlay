	pid {
		jump_speed_pid: jump_speed_pid {
			#controller-cells = <0>;
			compatible = "pid,single";
			k_p = "1.0";
			k_d = "0.0";
		};
	};
	emvalve {
		compatible = "gpio-keys";
		emvalve1: emvalve1 {
			gpios = <&gpiob 14 GPIO_ACTIVE_LOW>;//c板1口，推射
			label = "emvalve1";
		};
		emvalve2: emvalve2 {
			gpios = <&gpiob 15 GPIO_ACTIVE_LOW>;//c板3口，吸球
			label = "emvalve2";
		};
	};
	
	sbus0: sbus0 {
		compatible = "ares,sbus";
		status = "okay";
	};
};



&cdc_acm_uart0 {
	status = "disabled";
};

&spi1 {
    status = "okay";

    bmi08x_accel: bmi08x_accel@0 {
        compatible = "bosch,bmi08x-accel";
		status = "okay";
        reg = <0>;
        spi-max-frequency = <8000000>;
        int-gpios = <&gpioc 4 GPIO_ACTIVE_HIGH>;
        accel-hz = "1600";
        accel-fs = <12>;
		int1-map-io = <0>;
		int2-map-io = <0>;
        int1-conf-io = <0>;
		int2-conf-io = <0>;
		
    };
};

