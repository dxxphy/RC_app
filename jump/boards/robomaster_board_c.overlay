			tx_id = <0x3FE>;
			gear_ratio = "1.0";
			status = "okay";
			can_channel = <&can1>;
			controllers = <&jump_speed_pid>;
			capabilities = "speed";
			is_dm_motor;
			dm_i_max = "100";
			dm_torque_ratio = "-0.061538";
			follow = <&motor6>;
		};
